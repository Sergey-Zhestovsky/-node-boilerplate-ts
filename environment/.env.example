NODE_ENV=production

PORT=3000
HOST=localhost

# Logging
DEBUG='app:*'
LOGGING_FILE_LEVEL='info'
LOGGING_CONSOLE_LEVEL='info'

# Database connection
DB_URL='protocol://user:password@host:port/db_name'
DB_NAME='db_name'

# Auth
AUTH_SERVER_SECRET='secret'

# Swagger
SWAGGER=on
SWAGGER_SERVER_URL='http://localhost:3000/api/v1'

# AsyncAPI
ASYNCAPI=off
ASYNCAPI_PUBLIC_URL='ws://localhost:3000'